pipeline {
  agent any
  stages {
    stage('compile'){
      when { 'branch' 'main'}
      steps{
        echo "compile"
      }
    }
    stage('Test Cases'){
      when { expression { env.BRANCH_NAME != 'main'} }
      steps{
        echo "Test Case"
      }
    }
    stage('Docker Build'){
      steps{
        echo "Docker Build"
      }
    }
    stage('Docker Push'){
      steps{
        echo "Docker Push"
      }
    }
    stage('Deply to Dev'){
      steps{
        echo "Deply to Dev"
      }
    }
  }
}
